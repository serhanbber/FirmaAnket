@model IEnumerable<FirmaAnket.Models.AnswerLineDTO>
@{

    Layout = "~/Views/Shared/_Layout.cshtml";
    int number = 1;
}

<div class="card">






    <div class="card-body">
        <h4 class="text-center">Anket Detay Sayfası</h4>
        <div class="table-responsive">

            @foreach (var item in Model)
            {
                <span><b>@number)</b>@item.Question</span>
                <br /><span><b>@item.Answer</b></span>
                <br /><br /><br />
                number++;
            }

        </div>
    </div>
</div>



